{
  "name": "@uni-design-system/uni-react",
  "version": "0.0.1",
  "description": "React component library for the UNI Design System.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist/ && yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "storybook": "start-storybook -p 6006 -s ./.storybook/favicon",
    "build-storybook": "build-storybook -o docs -s ./.storybook/favicon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uni-design-system/uni-react.git"
  },
  "author": "George English",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/uni-design-system/uni-react/issues"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "ux",
    "design system"
  ],
  "homepage": "https://github.com/uni-design-system/uni-react#readme",
  "packageManager": "yarn@3.0.2",
  "dependencies": {
    "@uni-design-system/uni-core": "^0.0.21",
    "@uni-design-system/uni-react-icons": "^0.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@react-theming/storybook-addon": "^1.1.5",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-docs": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addons": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@storybook/theming": "^6.4.19",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "babel-loader": "^8.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
