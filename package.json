{
  "name": "@uni-design-system/uni-react",
  "version": "0.0.4",
  "description": "React component library for the UNI Design System.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist/ && yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "rm -rf docs/ && build-storybook -o docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uni-design-system/uni-react.git"
  },
  "author": "George English",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/uni-design-system/uni-react/issues"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "ux",
    "design system"
  ],
  "homepage": "https://github.com/uni-design-system/uni-react#readme",
  "packageManager": "yarn@3.0.2",
  "dependencies": {
    "@dnd-kit/core": "^6.0.7",
    "@dnd-kit/modifiers": "^6.0.1",
    "@dnd-kit/sortable": "^7.0.2",
    "@uni-design-system/uni-core": "^0.0.33",
    "@uni-design-system/uni-react-icons": "^0.1.1",
    "framer-motion": "4.1.17"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@react-theming/storybook-addon": "^1.1.6",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-docs": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-interactions": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/addons": "^6.5.6",
    "@storybook/react": "^6.5.6",
    "@storybook/testing-library": "^0.0.11",
    "@storybook/theming": "^6.5.6",
    "@types/node": "^18.14.6",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "babel-loader": "^8.2.3",
    "prettier": "2.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
